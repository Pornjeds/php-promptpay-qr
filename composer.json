
{
  "name": "kittinan/php-promptpay-qr",
  "description": "PHP Library to generate QR Code payload for PromptPay",
  "keywords": ["Promptpay"],
  "license": "MIT",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mermshaus/php-crc"
    }
  ],
  "require": {
    "php": ">=5.4",
    "bacon/bacon-qr-code": "1.0.1",
    "mermshaus/crc": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.12",
    "satooshi/php-coveralls": "v1.0.1"
  },
  "autoload": {
    "psr-4": {
      "KS\\": "src"
    }
  }
}